Ctrlp ClearCache	CtrlPClearCache
filetype:cpp	set ft=cpp
filetype:cs	set ft=cs
filetype:vim	set ft=vim
filetype:php	set ft=php
fileformat:dos	edit ++ff=dos
fileformat:unix	edit ++ff=unix
UnSyncScroll	set noscrollbind
Delete EOL Space	%s/\s\+$//ge
Reload VIMRC	source ~/.vim/.vimrc
PutDot	%s-\(//.*[^\.]\)$-\1.-
ChangeMidTab	%s/\v([^\t])\t+/\1 /ge
noexpandtab	set noexpandtab
check:php	!php -l %
